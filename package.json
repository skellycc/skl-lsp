{
  "name": "skl-lsp",
  "displayName": "skl-lsp",
  "description": "skelly programming language support",
  "version": "0.1.0",
  "publisher": "SkellyCompilerCollection",
  "repository": {
    "type": "git",
    "url": "git://github.com/skellycompilercollection/skl-lsp.git"
  },
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "skelly",
        "aliases": [
          "skelly",
          "skelly"
        ],
        "extensions": [
          ".skl",
          ".sklm"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "skelly",
        "scopeName": "source.skelly",
        "path": "./syntaxes/skelly.tmLanguage.json"
      }
    ]
  }
}
